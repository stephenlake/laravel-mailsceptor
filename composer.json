{
  "name": "stephenlake/laravel-mailsceptor",
  "description": "Intercept emails before they're submitted allowing you to redirect to alternative destinations, store output, view output via route and test development emails.",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "name": "Stephen Lake",
    "email": "stephen@closurecode.com"
  }],
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "illuminate/support": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
    "laravel/framework": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
    "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Mailsceptor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mailsceptor\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mailsceptor\\MailsceptorServiceProvider"
      ]
    }
  }
}
